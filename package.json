{
  "name": "mimic-chest-proxy",
  "version": "0.0.1",
  "description": "An easy reverse proxy to be used graphically",
  "productName": "Mimic Chest",
  "author": "Aarón Negrín <demenus.dev@gmail.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "dev:electron": "quasar dev -m electron",
    "build": "quasar build",
    "build:electron": "quasar build -m electron",
    "build:electron:win": "quasar build -m electron --target win32",
    "build:electron:linux": "quasar build -m electron --target linux",
    "build:electron:linux:appimage": "quasar build -m electron --target linux --appimage",
    "build:electron:mac": "quasar build -m electron --target darwin",
    "postinstall": "quasar prepare"
  },
  "dependencies": {
    "@quasar/cli": "^2.5.0",
    "@quasar/extras": "^1.16.4",
    "@types/http-proxy": "^1.17.17",
    "axios": "^1.2.1",
    "express": "^5.2.1",
    "express-http-proxy": "^2.1.2",
    "http-mitm-proxy": "^1.1.0",
    "monaco-editor-vue3": "^1.0.5",
    "parse-url": "^10.0.3",
    "picomatch": "^4.0.3",
    "pinia": "^3.0.1",
    "quasar": "^2.16.0",
    "vue": "^3.5.22",
    "vue-router": "^4.0.12",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@electron/packager": "^18.4.4",
    "@eslint/js": "^9.14.0",
    "@quasar/app-vite": "^2.1.0",
    "@types/express": "^4.17.21",
    "@types/express-http-proxy": "^1.6.7",
    "@types/node": "^20.5.9",
    "@types/picomatch": "^4.0.2",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "autoprefixer": "^10.4.2",
    "electron": "^39.2.6",
    "electron-builder": "^26.0.12",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.4.0",
    "prettier": "^3.3.3",
    "typescript": "^5.9.2",
    "vite-plugin-checker": "^0.11.0",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.7"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
